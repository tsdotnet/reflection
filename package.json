{
  "name": "@tsdotnet/reflection",
  "version": "1.0.18",
  "description": "",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/TypeValidator.d.ts",
  "main": "dist/TypeValidator.js",
  "module": "dist-esm/TypeValidator.js",
  "keywords": [
    "reflection",
    "type inspection",
    "type assertion",
    "runtime type",
    "runtime typechecking"
  ],
  "scripts": {
    "build:dist": "rimraf dist/* && tsc && copyfiles -u 1 src/**/*.d.ts dist",
    "build:dist-esm": "rimraf dist-esm/* && tsc -p tsconfig.esm.json",
    "build": "pnpm i && run-p build:*",
    "bump": "npm run precommit && npm version patch",
    "docs": "rimraf docs/* && rimraf docs/.nojekyll && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && run-p lint test && run-p build:* && npm run validate && git status -s",
    "prepublishOnly": "npm run build && run-p validate test",
    "preversion": "run-p lint test",
    "postversion": "git push && git push --tags && npm run docs && git commit -m \"Updated docs.\" docs && git push",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/reflection.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/reflection/issues"
  },
  "homepage": "https://github.com/tsdotnet/reflection#readme",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "eslint": "^7.27.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typedoc": "^0.20.36",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@tsdotnet/compare": "^1.3.17",
    "@tsdotnet/type": "^1.0.11",
    "tslib": "^2.2.0"
  }
}
